<section>
    <div class="container">
    <div class="row mt-4">
      <a href="/admin/add-products" class="btn btn-primary ml-auto">Add Products</a>
    </div>
    <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th>Image</th>
      <th scope="col">Brand</th>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
  {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td><img style="width: 50px;height: 50px;" src="{{this.image}}" alt=""></td>
      <td>{{this.brand}}</td>
      <td>{{this.name}}</td>
      <td>{{this.price}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
    </div>
    </section>